<div class= "row">
<br>
<center><h1> This Week's Box Office Top 10 </h1></center>
  <section class="small-6 small-centered columns">
      <table>
      <thead>
          <tr>
	          <th>Movie-Id </th>	
            <th>Movie</th>
            <th>Year</th>
          </tr>
      </thead>
        <tbody>
	
        <% @list2.each do | movie | %>
          <tr>  				
	           <td> <%= movie.imdb_id %></td>
	           <td> <a href="http://www.imdb.com/title/tt<%= movie.imdb_id %>/?ref_=cht_bo_"><%= movie.title %></a></td>
             <td><%= movie.year %> </td>
          </tr>
         <% end %>
	
	
        </tbody>
      </table>
  </section>
</div>
